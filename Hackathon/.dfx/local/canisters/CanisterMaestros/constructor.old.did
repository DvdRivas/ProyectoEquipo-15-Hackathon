type PreRecibo = 
 record {
   descripcion: text;
   matricula: text;
   tipo: nat;
 };
type Alumno = 
 record {
   carrera: text;
   historial: vec text;
   nombreCompleto: text;
   promedio: nat;
 };
service : {
  GenerarTransaccion: (PreRecibo) -> (text);
  InformacionDelAlumno: (text) -> (opt Alumno);
}
